<h1><%= @place["name"] %></h1>


<ul>
   <% for post in @posts %>
   <h2> <%= post["title"] %> </h2>
   <p> <%= post["posted_on"] %> </p>
    <%= post["description"] %>
    
   <% end %>
</ul>

<p>
  <a href="/posts/new?place_id=<%= @place["id"] %>">Create a new post</a>
</p>

<p><a href="/places/">Back to places </a></p>